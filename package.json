{
  "name": "@dasl/weasl",
  "type": "module",
  "version": "0.1.0",
  "description": "Warehouse that's Extensible for Arbitrary Structures & Links (WEASL) — @dasl/weasl — DASL store",
  "author": "Robin Berjon <robin@berjon.com>",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*",
      "import": "./dist/esm/*.js"
    },
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc && tsc-alias -p tsconfig.json",
    "postbuild": "esm2cjs --in dist/esm --out dist/cjs -l error",
    "release": "npm run build && release-it",
    "test": "npm run build && mocha"
  },
  "bin": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/darobin/weasl.git"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "mocha": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true,
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {}
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@alcalzone/esm2cjs": "^1.4.1",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^8.26.0",
    "mocha": "^11.7.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@atcute/cbor": "^2.2.5",
    "@atcute/cid": "^2.2.3"
  }
}
